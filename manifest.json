{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "browser_action": {
    "default_icon": "images/logo_16.png",
    "default_popup": "popup.html"
  },
  // "background": {
  //   "scripts": ["background.js"]
  // },
  "icons": { 
      "16": "images/logo_16.png",
      "48": "images/logo_48.png",
      "128": "images/logo_128.png" 
  }, 
  "permissions": [
      "activeTab"
  ], 
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.min.js", "content.js", "popup.js"],
      "css": ["abc.css"],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}


